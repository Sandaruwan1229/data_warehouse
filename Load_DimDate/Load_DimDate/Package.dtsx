                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
    CAST(FORMAT(rh.receipt_date, 'yyyyMMdd') AS INT) AS date_received_key,
    
    oh.vendor_id,
    od.product_id,
    rh.received_by_emp_id, 
    rh.location_id,
    
    CAST(rh.receipt_header_id AS VARCHAR(100)) AS receipt_no,
    r.qty_received,
    r.qty_accepted,
    r.qty_rejected,
    od.unit_price AS unit_price_at_receipt,
    
    CAST(FORMAT(oh.required_by_date, 'yyyyMMdd') AS INT) AS required_by_date_key,
    
    DATEDIFF(day, oh.required_by_date, rh.receipt_date) AS delivery_lateness_days,
    
    CASE 
        WHEN DATEDIFF(day, oh.required_by_date, rh.receipt_date) &lt;= 0 THEN 1 
        ELSE 0 
    END AS on_time_flag

